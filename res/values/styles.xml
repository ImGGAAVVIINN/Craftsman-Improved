<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Theme.MSA.DialogWhenLarge" parent="@android:style/Theme.Holo.DialogWhenLarge">
        <item name="android:actionBarStyle">@style/actionBar</item>
    </style>
    <style name="Theme.MSA.Dialog" parent="@android:style/Theme.Holo.Dialog">
        <item name="android:windowIsFloating">false</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowSoftInputMode">stateAlwaysHidden</item>
        <item name="android:actionBarStyle">@style/actionBar</item>
        <item name="android:windowActionModeOverlay">true</item>
    </style>
    <style name="Theme.MSA" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:actionBarStyle">@style/actionBar</item>
    </style>
    <style name="Theme.MSA.NoActionBar" parent="@android:style/Theme.Holo.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowActionBar">false</item>
    </style>
    <style name="Theme.MSA.Transparent" parent="@style/Theme.MSA">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:background">@android:color/transparent</item>
    </style>
    <style name="activity">
        <item name="android:background">@color/appBackground</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
    </style>
    <style name="actionBar" parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:background">@color/appBackground</item>
        <item name="android:divider">@null</item>
        <item name="android:titleTextStyle">@style/textHeader</item>
    </style>
    <style name="button" parent="@style/textLarge">
        <item name="android:textColor">@color/button_text</item>
        <item name="android:ellipsize">end</item>
        <item name="android:background">@drawable/button</item>
        <item name="android:paddingLeft">5.0dip</item>
        <item name="android:paddingRight">5.0dip</item>
        <item name="android:paddingBottom">3.0dip</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">@dimen/buttonHeight</item>
        <item name="android:minHeight">@dimen/buttonHeight</item>
        <item name="android:height">@dimen/buttonHeight</item>
        <item name="android:singleLine">true</item>
        <item name="android:layout_weight">1.0</item>
        <item name="font">SegoeUISemiBold</item>
    </style>
    <style name="dividerButtons">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">@dimen/marginLarge</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_weight">0.0</item>
    </style>
    <style name="overflowMenu">
        <item name="android:background">@drawable/msa_overflow_menu</item>
        <item name="android:layout_width">@dimen/menuWidth</item>
        <item name="android:layout_height">@dimen/menuHeight</item>
        <item name="android:layout_marginRight">@dimen/menuMarginRight</item>
        <item name="android:height">@dimen/menuHeight</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <style name="popupMessage" parent="@android:style/Theme.Translucent.NoTitleBar">
        <item name="android:backgroundDimAmount">0.5</item>
        <item name="android:layout_gravity">top</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="listView">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">fill_parent</item>
        <item name="android:layout_marginTop">@dimen/marginMedium</item>
        <item name="android:divider">@android:color/transparent</item>
        <item name="android:dividerHeight">@dimen/marginLarge</item>
        <item name="android:layout_alignParentLeft">true</item>
    </style>
    <style name="accountPickerListView" parent="@style/listView">
        <item name="android:layout_marginTop">@dimen/marginLarge</item>
        <item name="android:divider">@android:color/transparent</item>
        <item name="android:dividerHeight">@null</item>
    </style>
    <style name="userTile">
        <item name="android:background">@color/userTileBackground</item>
        <item name="android:paddingLeft">@dimen/marginLarge</item>
        <item name="android:paddingTop">@dimen/marginLarge</item>
        <item name="android:paddingRight">@dimen/marginLarge</item>
        <item name="android:paddingBottom">@dimen/marginLarge</item>
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="accountPickerUserTile" parent="@style/userTile">
        <item name="android:background">@null</item>
        <item name="android:paddingTop">@dimen/accountPickerMargin</item>
        <item name="android:paddingBottom">@dimen/accountPickerMargin</item>
    </style>
    <style name="userTileDisplayName" parent="@style/textLarge">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/marginMedium</item>
        <item name="android:layout_toRightOf">@id/imageUserTile</item>
        <item name="android:layout_alignTop">@id/imageUserTile</item>
    </style>
    <style name="userTileEmail" parent="@style/textDefault">
        <item name="android:layout_below">@id/textFirstLast</item>
        <item name="android:layout_alignLeft">@id/textFirstLast</item>
    </style>
    <style name="userTileImage">
        <item name="android:layout_width">@dimen/userTile</item>
        <item name="android:layout_height">@dimen/userTile</item>
        <item name="android:src">@drawable/msa_default_user_tile</item>
        <item name="android:layout_alignParentLeft">true</item>
        <item name="android:layout_alignParentTop">true</item>
        <item name="android:contentDescription">@string/user_tile_image_content_description</item>
    </style>
    <style name="userTileImageRtl" parent="@style/userTileImage">
        <item name="android:layout_alignParentLeft">false</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <style name="userTileImageBidirectional" parent="@style/userTileImage" />
    <style name="accountPickerUserTileUserName" parent="@style/textLarge">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">@dimen/userTile</item>
        <item name="android:layout_marginLeft">@dimen/marginMedium</item>
        <item name="android:layout_toRightOf">@id/imageUserTile</item>
    </style>
    <style name="accountPickerUserTileUserNameRtl" parent="@style/accountPickerUserTileUserName">
        <item name="android:layout_marginLeft">0.0dip</item>
        <item name="android:layout_marginRight">@dimen/marginMedium</item>
        <item name="android:layout_toLeftOf">@id/imageUserTile</item>
        <item name="android:layout_toRightOf">@null</item>
    </style>
    <style name="accountPickerUserTileUserNameBidirectional" parent="@style/accountPickerUserTileUserName" />
    <style name="accountTile" parent="@style/userTile">
        <item name="android:paddingTop">@dimen/marginLarge</item>
    </style>
    <style name="accountTileOverflowMenu">
        <item name="android:background">@android:color/transparent</item>
        <item name="android:layout_width">@dimen/userTile</item>
        <item name="android:layout_height">@dimen/userTile</item>
        <item name="android:src">@drawable/msa_overflow_menu</item>
        <item name="android:layout_alignParentRight">true</item>
    </style>
    <style name="textDefault">
        <item name="android:textSize">13.329987sp</item>
        <item name="android:textColor">#ff191919</item>
        <item name="android:background">#00000000</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:includeFontPadding">false</item>
        <item name="font">SegoeUI</item>
    </style>
    <style name="textLink" parent="@style/textDefault">
        <item name="isUnderlined">true</item>
    </style>
    <style name="textLarge" parent="@style/textDefault">
        <item name="android:textSize">16.0sp</item>
    </style>
    <style name="textLargest" parent="@style/textDefault">
        <item name="android:textSize">45.299988sp</item>
        <item name="font">SegoeUILight</item>
    </style>
    <style name="textHeader" parent="@style/textLarge">
        <item name="android:textColor">#ff585858</item>
        <item name="font">SegoeUISemiBold</item>
    </style>
    <style name="textStaticPage" parent="@style/textDefault">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginLeft">@dimen/marginLarge</item>
        <item name="android:layout_marginRight">@dimen/marginLarge</item>
        <item name="android:layout_marginBottom">@dimen/marginXLarge</item>
    </style>
    <style name="textPopupMessageHeader" parent="@style/textHeader">
        <item name="android:textSize">20.599976sp</item>
        <item name="android:textColor">@color/popupMessageText</item>
    </style>
    <style name="textPopupMessageBody" parent="@style/textLarge">
        <item name="android:textColor">@color/popupMessageText</item>
    </style>
    <style name="signOutCheckBox">
        <item name="android:layout_width">fill_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:checked">false</item>
    </style>
    <style name="signOutCheckBoxRtl" parent="@style/signOutCheckBox">
        <item name="android:button">@null</item>
        <item name="android:drawableRight">?android:listChoiceIndicatorMultiple</item>
    </style>
    <style name="signOutCheckBoxBidirectional" parent="@style/signOutCheckBox" />
    <style name="Base">
        <item name="android:textColorLink">@color/xbid_white_primary</item>
    </style>
    <style name="Header" parent="@style/Base">
        <item name="android:ellipsize">end</item>
    </style>
    <style name="Header.UserName" parent="@style/Header">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="Header.Email" parent="@style/Header">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/xbid_white_secondary</item>
    </style>
    <style name="Body" parent="@style/Base">
        <item name="android:textColor">@color/xbid_white_primary</item>
        <item name="android:ellipsize">end</item>
    </style>
    <style name="Body.Title" parent="@style/Body">
        <item name="android:textSize">22.0sp</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="Body.TitleComment" parent="@style/Body">
        <item name="android:textSize">18.0sp</item>
    </style>
    <style name="Body.TextInput" parent="@style/Body">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
        <item name="android:textColorHint">@color/xbid_white_secondary</item>
        <item name="android:background">@color/xbid_button_background</item>
        <item name="android:padding">3.0dip</item>
        <item name="android:singleLine">true</item>
    </style>
    <style name="Body.TextInputComment" parent="@style/Body.TitleComment" />
    <style name="Body.BottomButton" parent="@style/Body.TitleComment">
        <item name="android:background">@drawable/xbid_button_background</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="Body.GamerTag" parent="@style/Body">
        <item name="android:textSize">18.0sp</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="Body.UserName" parent="@style/Body">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="Body.SectionTitle" parent="@style/Body">
        <item name="android:textSize">14.0sp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="Body.SectionText" parent="@style/Body">
        <item name="android:textSize">12.0sp</item>
        <item name="android:textColor">@color/xbid_white_secondary</item>
    </style>
    <style name="Body.SuggestionText" parent="@style/Body.TextInputComment" />
    <style name="Error" parent="@style/Body">
        <item name="android:textSize">20.0sp</item>
        <item name="android:textColor">@color/xbid_white_primary</item>
    </style>
    <style name="Error.Greeting" parent="@style/Error" />
    <style name="Error.Normal" parent="@style/Error" />
    <style name="Error.Button" parent="@style/Error">
        <item name="android:background">@drawable/xbid_button_background</item>
        <item name="android:textAllCaps">false</item>
    </style>
    <style name="OnlineidUiTheme" parent="@android:style/Theme.Holo.Light.NoActionBar">
        <item name="android:windowBackground">@color/xbid_activity_background</item>
    </style>
</resources>
